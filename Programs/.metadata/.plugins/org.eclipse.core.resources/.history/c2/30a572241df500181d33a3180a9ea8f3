package com.bridgelabz.functional;

public class PrimeNumber {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
	
		int n = Integer.parseInt(args[0]);
		for(int i = 0; i < n; i++) {
			boolean status = true;
			for(int j= 2; j < i; j++) {
				while(i % j == 0) {
				  status = false;
				  break;
				}
			}
			if(status) {
				System.out.println(" " + i);
			}
		}

	}

}
