package com.bridgelabz.functional;

public class Factors {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int n = Integer.parseInt(args[0]);
		int factor = 1;
		
		for(int i = 1; i < n; i++ ) {
			
			factor = factor * i;
			
			System.out.println("Factorial of " + n + " are = " + factor);
		}
		

	}

}
